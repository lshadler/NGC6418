  </PARAM>

  <PARAM id="expid" type="string" default="">
   <DESCRIPTION>
    Exposure identifier
   </DESCRIPTION>
   <CONSTRAINTS>
    true
   </CONSTRAINTS>
  </PARAM>

  <PARAM id="granularity" type="real" default="10">
   <DESCRIPTION>
    Time granularity of the HK data
   </DESCRIPTION>
   <CONSTRAINTS>
    true
   </CONSTRAINTS>
  </PARAM>

  <PARAM id="filter" type="string" default="UNKNOWN">
   <DESCRIPTION>
    Filter Name
