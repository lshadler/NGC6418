<!-- converted from XPI formatted "phkgen.par.xpi"-->
<FILE>
 <CONFIG>

  <PARAM id="phkf" type="file" default="PHKF.FIT">
   <DESCRIPTION>
    Name of the PHKF
   </DESCRIPTION>
   <CONSTRAINTS>
    true
   </CONSTRAINTS>
  </PARAM>

  <PARAM id="tstart" type="string" default="1998-01-01T00:00:00">
   <DESCRIPTION>
    Start time
   </DESCRIPTION>
   <CONSTRAINTS>
    true
   </CONSTRAINTS>
  </PARAM>

  <PARAM id="tstop" type="string" default="2000-01-21T00:00:00">
   <DESCRIPTION>
    End time
   </DESCRIPTION>
   <CONSTRAINTS>
    true
   </CONSTRAINTS>
  </PARAM>

  <PARAM id="instrument" type="string" default="EMOS1">
   <DESCRIPTION>
    Instrument
   </DESCRIPTION>
   <CONSTRAINTS>
    true
   </CONSTRAINTS>
  </PARAM>

  <PARAM id="extname" type="string" default="PERIODIC_HK">
