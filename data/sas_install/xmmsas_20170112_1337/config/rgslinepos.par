   </CONSTRAINTS>
  </PARAM>

  <PARAM id="ocb" type="int" default="3">
   <DESCRIPTION>
    On-chip binning
   </DESCRIPTION>
   <CONSTRAINTS>
    ocb in [1:5]
   </CONSTRAINTS>
  </PARAM>

 </CONFIG>
</FILE>
