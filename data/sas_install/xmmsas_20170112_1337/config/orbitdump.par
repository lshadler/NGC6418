<!-- orbitdump parameter file -->
<FILE>
<CONFIG>
	<PARAM id="tstep" type="real" default="10">
		<DESCRIPTION>time increment [s]</DESCRIPTION>
	</PARAM>

	<PARAM id="offsett" type="bool" default="no">
		<DESCRIPTION>Offset all times by time of first element?</DESCRIPTION>
		<CASE>
			<ITEM value="no"/>
			<ITEM value="yes"/>
		</CASE>
	</PARAM>
</CONFIG>
</FILE>
